<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>{% block title %}{% endblock %} - 香港庆东集团 - 大连庆东美容整形公司</title>
    <!-- Basic Styles -->

    <link rel="stylesheet" type="text/css" media="screen" href="{{ asset((qd_admin_url~"css/bootstrap.min.css")) }}">
    <link rel="stylesheet" type="text/css" media="screen" href="{{ asset((qd_admin_url~"css/font-awesome.min.css")) }}">
    <link rel="stylesheet" type="text/css" media="screen" href="{{ asset((qd_admin_url~"css/smartadmin-production_unminified.css")) }}">
    <link rel="stylesheet" type="text/css" media="screen" href="{{ asset((qd_admin_url~"css/smartadmin-skins.css")) }}">
    <!-- Demo purpose only: goes with demo.js, you can delete this css when designing your own WebApp -->
    <link rel="stylesheet" type="text/css" media="screen" href="{{ asset((qd_admin_url~"css/demo.css")) }}">
    <!-- FAVICONS -->
    <link rel="shortcut icon" href="{{ asset((qd_admin_url~"img/favicon/favicon.ico")) }}" type="image/x-icon">
    <link rel="icon" href="{{ asset((qd_admin_url~"img/favicon/favicon.ico")) }}" type="image/x-icon">

    {% block stylesheets %}{% endblock %}
</head>


<body id="login" class="animated fadeInDown">
{% block header %}
    <header id="header">
        <!--<span id="logo"></span>-->
        <div id="logo-group">
            <span id="logo"><a href="http://www.qingdongprs.com" target="_blank" title="香港庆东集团 - 大连庆东美容整形公司"><img src="{{asset((qd_admin_url~"img/logo-login.png")) }}" alt="香港庆东集团 - 大连庆东美容整形公司"></a></span>
        </div>
    </header>
{% endblock %}

{% block main %}
{% endblock %}



{% block footer %}
    <!--================================================== -->

    <!-- PACE LOADER - turn this on if you want ajax loading to show (caution: uses lots of memory on iDevices)-->
    <script src="{{ asset((qd_admin_url~"js/plugin/pace/pace.min.js")) }}"></script>

    <script src="{{ asset((qd_admin_url~"js/libs/jquery-2.0.2.min.js")) }}"></script>
    <script src="{{ asset((qd_admin_url~"js/libs/jquery-ui-1.10.3.min.js")) }}"></script>

    <!-- JS TOUCH : include this plugin for mobile drag / drop touch events
    <script src="{{ asset((qd_admin_url~"js/plugin/jquery-touch/jquery.ui.touch-punch.min.js")) }}"></script> -->

    <!-- BOOTSTRAP JS -->
    <script src="{{ asset((qd_admin_url~"js/bootstrap/bootstrap.min.js")) }}"></script>

    <!-- CUSTOM NOTIFICATION -->
    <script src="{{ asset((qd_admin_url~"js/notification/SmartNotification.min.js")) }}"></script>

    <!-- JARVIS WIDGETS -->
    <script src="{{ asset((qd_admin_url~"js/smartwidgets/jarvis.widget.min.js")) }}"></script>

    <!-- EASY PIE CHARTS -->
    <script src="{{ asset((qd_admin_url~"js/plugin/easy-pie-chart/jquery.easy-pie-chart.min.js")) }}"></script>

    <!-- SPARKLINES -->
    <script src="{{ asset((qd_admin_url~"js/plugin/sparkline/jquery.sparkline.min.js")) }}"></script>

    <!-- JQUERY VALIDATE -->
    <script src="{{ asset((qd_admin_url~"js/plugin/jquery-validate/jquery.validate.min.js")) }}"></script>

    <!-- JQUERY MASKED INPUT -->
    <script src="{{ asset((qd_admin_url~"js/plugin/masked-input/jquery.maskedinput.min.js")) }}"></script>

    <!-- JQUERY SELECT2 INPUT -->
    <script src="{{ asset((qd_admin_url~"js/plugin/select2/select2.min.js")) }}"></script>

    <!-- JQUERY UI + Bootstrap Slider -->
    <script src="{{ asset((qd_admin_url~"js/plugin/bootstrap-slider/bootstrap-slider.min.js")) }}"></script>

    <!-- browser msie issue fix -->
    <script src="{{ asset((qd_admin_url~"js/plugin/msie-fix/jquery.mb.browser.min.js")) }}"></script>

    <!-- FastClick: For mobile devices -->
    <script src="{{ asset((qd_admin_url~"js/plugin/fastclick/fastclick.js")) }}"></script>

    <!--[if IE 7]>
    <h1>Your browser is out of date, please update your browser by going to www.microsoft.com/download</h1>
    <![endif]-->

    <!-- MAIN APP JS FILE -->
    <script src="{{ asset((qd_admin_url~"js/app.js")) }}"></script>

    <script type="text/javascript">
        runAllForms();
        $(function() {
            // Validation
            $("#login-form").validate({
                // Rules for form validation
                rules : {
                    _username : {
                        required : true
                    },
                    _password : {
                        required : true,
                        minlength : 3,
                        maxlength : 20
                    }
                },

                // Messages for form validation
                messages : {
                    _username : {
                        required : '请输入您的登录用户名'
                    },
                    _password : {
                        required : '请输入您的登录密码'
                    }
                },

                // Do not change code below
                errorPlacement : function(error, element) {
                    error.insertAfter(element.parent());
                }
            });
        });
    </script>
{% endblock %}
</body>
</html>